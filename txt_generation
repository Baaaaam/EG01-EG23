cyan -db cyclus.sqlite power -proto=lwr_A > Energy_lwr_A.txt
cyan -db cyclus.sqlite power -proto=lwr_B > Energy_lwr_B.txt
cyan -db cyclus.sqlite power -proto=SFR_A > Energy_SFR_A.txt
cyan -db cyclus.sqlite power -proto=SFR_B > Energy_SFR_B.txt
cyan -db cyclus.sqlite table Inventories > full_inventory_by_agent.txt
cyan -db cyclus.sqlite table TimeSeriesEnrichmentFeed > TimeSeriesEnrichmentFeed.txt
cyan -db cyclus.sqlite flow -to SFR_A_separations > SFR_A_separations.txt
cyan -db cyclus.sqlite flow -to SFR_B_separations > SFR_B_separations.txt
cyan -db cyclus.sqlite flow -to LWR_separations > LWR_separations.txt
cyan -db cyclus.sqlite inv LWR_storage_A > LWR_storage_A.txt
cyan -db cyclus.sqlite inv LWR_storage_B > LWR_storage_B.txt
cyan -db cyclus.sqlite inv SFR_A_storage > SFR_A_storage.txt
cyan -db cyclus.sqlite inv SFR_B_storage > SFR_B_storage.txt

cyan -db cyclus.sqlite flow -to SFR_A > SFR_A.txt
cyan -db cyclus.sqlite flow -to SFR_B > SFR_B.txt
cyan -db cyclus.sqlite flow -to lwr_A > LWR_A.txt
cyan -db cyclus.sqlite flow -to lwr_B > LWR_B.txt