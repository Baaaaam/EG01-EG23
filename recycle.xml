  <prototype>
    <name>Separation</name>
    <config>
      <Separations>
        <leftover_commod>waste</leftover_commod>
        <throughput>349650</throughput>
        <feedbuf_size>400000</feedbuf_size>
        <feed_commods>
          <val>UOX_A_spent_stored</val>
          <val>UOX_B_spent_stored</val>
          <val>SFR_A_spent_stored</val>
          <val>SFR_B_spent_stored</val>
        </feed_commods>
        <feed_commod_prefs>
          <val>4.0</val>
          <val>3.0</val>
          <val>2.0</val>
          <val>1.0</val>
        </feed_commod_prefs>
        <streams>
          <item>
            <commod>pu</commod>
            <info>
              <buf_size>5000</buf_size>
              <efficiencies>
                <item> <comp>Pu</comp> <eff>.99</eff> </item>
              </efficiencies>
            </info>
          </item>
        </streams>

      </Separations>
    </config>
  </prototype>


  <prototype>
    <name>waste</name>
    <config>
      <Sink>
        <in_commods><val>waste</val></in_commods>
        <capacity>1e100</capacity>
      </Sink>
    </config>
  </prototype>

  <prototype>
    <name>depleted_src</name>
    <config>
      <Source>
        <outcommod>depleted_u</outcommod>
        <outrecipe>depleted_u</outrecipe>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>

  <prototype>
    <name>deployer</name>
    <config>
      <DeployInst>
        <prototypes>
          <val>waste</val>
          <val>enrichment</val>
          <val>depleted_src</val>
        </prototypes>

        <build_times>
          <val>1</val>
          <val>1</val>
          <val>1</val>
        </build_times>

        <n_build>
          <val>1</val>
          <val>1</val>
          <val>1</val>
        </n_build>
      </DeployInst>
    </config>
  </prototype>


