<My_include>
  
  <!-- Comodity list & definition:
   
   * FBR_driver_MOX_new    : new MOX driver fuel for FBR
   * FBR_driver_MOX_spent  : spent MOX driver fuel for FBR
   * FBR_driver_MOX_cooled : spent MOX driver fuel for FBR AFTER COOLING
   * FBR_driver_MOX_stored : spent MOX driver fuel for FBR AVAILABLE in storage
   
   * FBR_blanket_U_new    : new U blanket fuel for FBR
   * FBR_blanket_U_spent  : spent U blanket fuel for FBR
   * FBR_blanket_U_cooled : spent U blanket fuel for FBR AFTER COOLING
   * FBR_blanket_U_stored : spent U blanket fuel for FBR AVAILABLE in storage
   
   
   * FBR_driver_pu : pu from driver after separation
   * FBR_driver_ru : ru from driver after separation
   * FBR_driver_pu_stored : pu from FBR driver AVAILABLE in storage
   * FBR_driver_ru_stored : ru from FBR driver AVAILABLE in storage
   
   * FBR_blanket_pu : pu from blanket after separation
   * FBR_blanket_ru : ru from blanket after separation
   * FBR_blanket_pu_stored : pu from FBR blanket AVAILABLE in storage
   * FBR_blanket_ru_stored : ru from FBR blanket AVAILABLE in storage
   
   
   * FBR_ma : ma after separation
   * FBR_fp : fp after separation
   * FBR_ma_stored : ma from FBR AVAILABLE in storage
   * FBR_fp_stored : fp from FBR AVAILABLE in storage
   
   * EXT1_blanket_pu_stored : pu form EXT source 1
   * EXT1_ru_stored         : ru from EXT source 1
   
   -->
  
  
  <!--      FBR fabrication      -->
  <prototype>
    <name>Mixing_Fab</name>
    <config>
      <MixingFab>
        
        <in_commods>
          <val>FBR_blanket_upu_E2_stored</val>
          <val>FBR_driver_pu_E1_stored</val>
        </in_commods>
        
        <commods_size>
          <val>100000</val>
          <val>100000</val>
        </commods_size>

        <commods_frac>
          <val>0.141146</val>
          <val>0.858854</val>
        </commods_frac>


        <throughput>1000000</throughput>
        
        <outcommod>MIX</outcommod>
        <outputbuf_size>10000000</outputbuf_size>
        
      </MixingFab>
    </config>
  </prototype>
  
  <!--       FBR blanket Storage       -->
  <prototype>
    <name>Mix_storage</name>
    <config>
      <Storage>
        <in_commods> <val>MIX</val></in_commods>
        <out_commods> <val>MIX_Stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  
  
  
  <!-- *********************** -->
  <prototype>
    <name>FBR_fabrication</name>
    <config>
      <FuelFab>
        
        <fill_commods>
          <val>natl_u</val>
        </fill_commods>
        
        <fill_recipe>natl_u</fill_recipe>
        <fill_size>100000</fill_size>
        
        <fiss_commods>
          <val>FBR_driver_MOX_new</val>
        </fiss_commods>
        
        <fiss_commod_prefs>
          <val>1.0</val>
        </fiss_commod_prefs>
        
        <fiss_size>100000</fiss_size>
        
        <throughput>1000000</throughput>
        <spectrum>fission_spectrum_ave</spectrum>
        <outcommod>test</outcommod>
        
      </FuelFab>
    </config>
  </prototype>
  
  
  <prototype>
    <name>FBR_blanket_fabrication</name>
    <config>
      <FuelFab>
        
        <fill_commods>
          <val>FBR_blanket_u_RU_stored</val>
        </fill_commods>
        
        <fill_recipe>FBR_blanket_u_RU</fill_recipe>
        <fill_size>100000</fill_size>
        
        <fiss_commods>
          <val>natl_u</val>
        </fiss_commods>
        
        <fiss_commod_prefs>
          <val>1.0</val>
        </fiss_commod_prefs>
        
        <fiss_size>100000</fiss_size>
        
        <throughput>1000000</throughput>
        <spectrum>fission_spectrum_ave</spectrum>
        <outcommod>FBR_blanket_new</outcommod>
        
      </FuelFab>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--       FBR reactor       -->
  <prototype>
    <name>FBR_driver</name>
    <lifetime>960</lifetime>
    <config>
      <Reactor>
        <fuel_inrecipes>  <val>FBR_driver_MOX_new</val></fuel_inrecipes>
        <fuel_incommods>  <val>FBR_driver_MOX_new</val></fuel_incommods>
        
        <fuel_outrecipes> <val>FBR_driver_MOX_spent</val></fuel_outrecipes>
        <fuel_outcommods> <val>FBR_driver_MOX_spent</val></fuel_outcommods>
        
        <cycle_time>9</cycle_time>
        <refuel_time>0</refuel_time>
        <assem_size>14784</assem_size>
        <n_assem_core>6</n_assem_core>
        <n_assem_batch>1</n_assem_batch>
        <power_cap>900</power_cap>
        
      </Reactor>
    </config>
  </prototype>
  
  <prototype>
    <name>FBR_blanket</name>
    <lifetime>960</lifetime>
    <config>
      <Reactor>
        <fuel_inrecipes>  <val>FBR_blanket_new</val></fuel_inrecipes>
        <fuel_incommods>  <val>FBR_blanket_new</val></fuel_incommods>
        
        <fuel_outrecipes> <val>FBR_blanket_spent</val></fuel_outrecipes>
        <fuel_outcommods> <val>FBR_blanket_spent</val></fuel_outcommods>
        
        <cycle_time>9</cycle_time>
        <refuel_time>0</refuel_time>
        <assem_size>14784</assem_size>
        <n_assem_core>6</n_assem_core>
        <n_assem_batch>1</n_assem_batch>
        <power_cap>900</power_cap>
        
      </Reactor>
    </config>
  </prototype>


  
  <!-- *********************** -->
  
  <!--       FBR driver Cooling       -->
  <prototype>
    <name>FBR_driver_MOX_cooling</name>
    <config>
      <Storage>
        <in_commods> <val>FBR_driver_MOX_spent</val></in_commods>
        <out_commods> <val>FBR_driver_MOX_cooled</val></out_commods>
        <residence_time>84</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--       FBR driver Storage       -->
  <prototype>
    <name>FBR_driver_MOX_storage</name>
    <config>
      <Storage>
        <in_commods> <val>FBR_driver_MOX_cooled</val></in_commods>
        <out_commods> <val>FBR_driver_MOX_stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--      FBR driver Separation     -->
  <prototype>
    <name>FBR_driver_separation</name>
    <config>
      <Separations>
        <feedbuf_size>107537</feedbuf_size>
        
        <feed_commods>
          <val>FBR_driver_MOX_stored</val>
        </feed_commods>
        
        <feed_commod_prefs>
          <val>1.0</val>
        </feed_commod_prefs>
        
        <throughput>83333.3333</throughput>
        
        <streams>
          <item>
            <commod>FBR_driver_upu_E1</commod>
            <info>
              <buf_size>1e100</buf_size>
              <efficiencies>
                <item> <comp>Pu</comp> <eff>.988</eff> </item>
                <item> <comp>U</comp> <eff>.988</eff> </item>
              </efficiencies>
            </info>
          </item>
        </streams>
        
        <leftover_commod>waste</leftover_commod>
        
      </Separations>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--      FBR driver pu storage     -->
  <prototype>
    <name>FBR_driver_storage_upu_E1</name>
    <config>
      <Storage>
        <in_commods>
          <val>FBR_driver_upu_E1</val>
          <val>FBR_driver_upu_E1_initial</val>
        </in_commods>
        <out_commods> <val>FBR_driver_pu_E1_stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  
  <!--       FBR blanket Cooling       -->
  <prototype>
    <name>FBR_blanket_cooling</name>
    <config>
      <Storage>
        <in_commods> <val>FBR_blanket_spent</val></in_commods>
        <out_commods> <val>FBR_blanket_cooled</val></out_commods>
        <residence_time>84</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--       FBR blanket Storage       -->
  <prototype>
    <name>FBR_blanket_storage</name>
    <config>
      <Storage>
        <in_commods> <val>FBR_blanket_cooled</val></in_commods>
        <out_commods> <val>FBR_blanket_stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--      FBR blanket Separation     -->
  <prototype>
    <name>FBR_blanket_separation</name>
    <config>
      <Separations>
        <feedbuf_size>107537</feedbuf_size>
        
        <feed_commods>
          <val>FBR_blanket_stored</val>
        </feed_commods>
        
        <feed_commod_prefs>
          <val>1.0</val>
        </feed_commod_prefs>
        
        <throughput>83333.3333</throughput>
        
        <streams>
          <item>
            <commod>FBR_blanket_upu_E2</commod>
            <info>
              <buf_size>1e100</buf_size>
              <efficiencies>
                <item> <comp>Pu</comp> <eff>0.388898</eff> </item>
                <item> <comp>U</comp> <eff>0.1</eff> </item>
              </efficiencies>
            </info>
          </item>
          <item>
            <commod>FBR_blanket_upu_E3</commod>
            <info>
              <buf_size>1e100</buf_size>
              <efficiencies>
                <item> <comp>Pu</comp> <eff>0.590746</eff> </item>
                <item> <comp>U</comp> <eff>0.1</eff> </item>
              </efficiencies>
            </info>
          </item>
          <item>
            <commod>FBR_blanket_u_RU</commod>
            <info>
              <buf_size>1e100</buf_size>
              <efficiencies>
                <item> <comp>U</comp> <eff>0.1</eff> </item>
              </efficiencies>
            </info>
          </item>
        </streams>
        
        <leftover_commod>waste</leftover_commod>
        
      </Separations>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--      FBR blanket E2 storage     -->
  <prototype>
    <name>FBR_blanket_storage_E2</name>
    <config>
      <Storage>
        <in_commods>
          <val>FBR_blanket_upu_E2</val>
          <val>FBR_blanket_upu_E2_initial</val>
        </in_commods>
        <out_commods> <val>FBR_blanket_upu_E2_stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <!--      FBR blanket E3 storage     -->
  <prototype>
    <name>FBR_blanket_storage_E3</name>
    <config>
      <Storage>
        <in_commods> <val>FBR_blanket_upu_E3</val></in_commods>
        <out_commods> <val>FBR_blanket_upu_E3_stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  <!-- *********************** -->
  
  <prototype>
    <name>FBR_blanket_storage_RU</name>
    <config>
      <Storage>
        <in_commods>
          <val>FBR_blanket_u_RU_initial</val>
          <val>FBR_blanket_u_RU</val>
        </in_commods>
        <out_commods> <val>FBR_blanket_u_RU_stored</val></out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </prototype>
  
  
  
  
  
</My_include>
