<simulation xmlns:xi="http://www.w3.org/2001/XInclude">

  <schematype>flat</schematype>
  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
    <startyear>2015</startyear>
    <decay>never</decay>
  </control>

  <archetypes>
    <spec> <lib>cycamore</lib> <name>Source</name></spec>
    <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
    <spec> <lib>cycamore</lib> <name>Sink</name></spec>
    <spec> <lib>cycamore</lib> <name>Enrichment</name> </spec>
    <spec> <lib>cycamore</lib> <name>Reactor</name> </spec>
    <spec> <lib>cycamore</lib> <name>Storage</name> </spec>
    <spec> <lib>cycamore</lib> <name>FuelFab</name> </spec>
    <spec> <lib>cycamore</lib> <name>Separations</name> </spec>
    <spec> <lib>cycamore</lib> <name>Mixer</name> </spec>
  </archetypes>


  <xi:include href="recipe.xml" xpointer="xpointer(//My_include/*)"/>
  <xi:include href="fbr_cycle.xml" xpointer="xpointer(//My_include/*)"/>
  <xi:include href="pwr_cycle.xml" xpointer="xpointer(//My_include/*)"/>
  <xi:include href="pwr_deployement.xml" xpointer="xpointer(//My_include/*)"/>
  <xi:include href="fbr_deployement.xml" xpointer="xpointer(//My_include/*)"/>
  <xi:include href="fbr_blanket_deployement.xml" xpointer="xpointer(//My_include/*)"/>
  
  <prototype>
    <name>natl_u_source</name>
    <config>
      <Source>
	<outcommod>natl_u</outcommod>
	<outrecipe>natl_u</outrecipe>

	<inventory_size>1e100</inventory_size>
	<throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>
  
  <prototype>
    <name>J1_second_limited_source</name>
    <config>
      <Source>
	<outcommod>J1_second_initial</outcommod>
	<outrecipe>J1</outrecipe>

	<inventory_size>1931000</inventory_size>
	<throughput>250000</throughput>
      </Source>
    </config>
  </prototype>

  <prototype>
    <name>E1_second_limited_source</name>
    <config>
      <Source>
	<outcommod>E1_second_initial</outcommod>
	<outrecipe>E1</outrecipe>
	<inventory_size>2400000</inventory_size>
	<throughput>2000000</throughput>
      </Source>
    </config>
  </prototype>

  <prototype>
    <name>E2_second_limited_source</name>
    <config>
      <Source>
	<outcommod>E2_second_initial</outcommod>
	<outrecipe>E2</outrecipe>

	<inventory_size>560000</inventory_size>
	<throughput>200000</throughput>
      </Source>
    </config>
  </prototype>

  <prototype>
    <name>E3_second_limited_source</name>
    <config>
      <Source>
        <outcommod>E3_second_initial</outcommod>
        <outrecipe>E3</outrecipe>

        <inventory_size>381000</inventory_size>
        <throughput>200000</throughput>
      </Source>
    </config>
  </prototype>

  <prototype>
    <name>E4_limited_source</name>
    <config>
      <Source>
	<outcommod>E4_initial</outcommod>
	<outrecipe>E4</outrecipe>

	<inventory_size>6360000</inventory_size>
	<throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>


<prototype>
  <name>waste</name>
  <config>
    <Sink>
      <in_commods><val>waste</val></in_commods>
      <capacity>1e100</capacity>
    </Sink>
  </config>
</prototype>

  
  
  
</simulation>
