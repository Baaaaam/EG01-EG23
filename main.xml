<simulation xmlns:xi="http://www.w3.org/2001/XInclude">
  
  <schematype>flat</schematype>
  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
    <startyear>2015</startyear>
    <decay>never</decay>
  </control>
  
  <archetypes>
    <spec> <lib>cycamore</lib> <name>Source</name></spec>
    <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
    <spec> <lib>cycamore</lib> <name>Sink</name></spec>
    <spec> <lib>cycamore</lib> <name>Enrichment</name> </spec>
    <spec> <lib>cycamore</lib> <name>Reactor</name> </spec>
    <spec> <lib>cycamore</lib> <name>Storage</name> </spec>
    <spec> <lib>cycamore</lib> <name>FuelFab</name> </spec>
    <spec> <lib>cycamore</lib> <name>Separations</name> </spec>
  </archetypes>
  
  
  <xi:include href="recipe.xml" xpointer="xpointer(//My_include/*)"/>
  <xi:include href="SFR-cycle.xml" xpointer="xpointer(//My_include/*)"/>
  <!--        <xi:include href="LWR-cycle.xml" xpointer="xpointer(//My_include/*)"/> -->
  
  <prototype>
    <name>natl_u_source</name>
    <config>
      <Source>
        <outcommod>natl_u</outcommod>
        <outrecipe>natl_u</outrecipe>
        
        <inventory_size>1e100</inventory_size>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>
  
  
  <prototype>
    <name>blanket_Upu_limited_E2_source</name>
    <config>
      <Source>
        <outcommod>FBR_blanket_upu_E2_initial</outcommod>
        <outrecipe>FBR_blanket_upu_E2</outrecipe>
        
        <inventory_size>1000000</inventory_size>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>
  
  <prototype>
    <name>FBR_Upu_limited_E1_source</name>
    <config>
      <Source>
        <outcommod>FBR_driver_upu_E1_initial</outcommod>
        <outrecipe>FBR_driver_upu_E1</outrecipe>
        
        <inventory_size>1000000</inventory_size>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>
  
  <prototype>
    <name>FBR_blanket_u_limited_RU_source</name>
    <config>
      <Source>
        <outcommod>FBR_blanket_u_RU_initial</outcommod>
        <outrecipe>FBR_blanket_u_RU</outrecipe>
        
        <inventory_size>1000000</inventory_size>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>
  
  <prototype>
    <name>waste</name>
    <config>
      <Sink>
        <in_commods><val>waste</val></in_commods>
        <capacity>1e100</capacity>
      </Sink>
    </config>
  </prototype>
  
  <prototype>
    <name>deployer_FBR_CHAIN</name>
    <config>
      <DeployInst>
        <prototypes>
          <val>waste</val>
          
          <val>natl_u_source</val>
          <val>blanket_Upu_limited_E2_source</val>
          <val>FBR_Upu_limited_E1_source</val>
          <val>FBR_blanket_u_limited_RU_source</val>
          
          
          <val>FBR_driver_upu_repro_mix_fabrication</val>
          
          
          <val>FBR_blanket_fabrication</val>
          <val>FBR_driver_MOX_cooling</val>
          <val>FBR_driver_MOX_storage</val>
          <val>FBR_driver_separation</val>
          <val>FBR_fabrication</val>
          
          <!--
           
           
           
           -->
          <val>FBR_driver_storage_upu_E1</val>
          
          <val>FBR_blanket_cooling</val>
          <val>FBR_blanket_storage</val>
          <val>FBR_blanket_separation</val>
          <val>FBR_blanket_storage_E2</val>
          <val>FBR_blanket_storage_E3</val>
          <val>FBR_blanket_storage_RU</val>
          <!--
           
           -->
          
          <val>FBR_driver</val>
          <val>FBR_blanket</val>
        </prototypes>
        
        <build_times>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
